//
// Paintball.ProjectileWeaponInfo
//
@BaseClass = pweap : ""
[
	followeffect(resource:vpcf) { group = "VFX" } : "Follow Effect" :  : ""
	firesound(string) { group = "Sounds" } : "Fire Sound" :  : ""
	projectilemodel(resource:vmdl) { group = "Models" } : "Projectile Model" : "models/paintball/paintball.vmdl" : ""
	clipsize(integer) { group = "Stats" } : "Clip Size" : 0 : ""
	bulletsperfire(integer) { group = "Stats" } : "Bullets Per Fire" : 1 : ""
	firemode(choices) { group = "Stats" } : "Fire Mode" : 0 : "" =
	[
		0 : "Automatic"
		1 : "Semi"
		2 : "Burst"
	]
	primaryrate(float) { group = "Stats" } : "Primary Rate" : "5" : ""
	projectilegravity(float) { group = "Stats" } : "Projectile Gravity" : 0 : ""
	projectileradius(float) { group = "Stats" } : "Projectile Radius" : "3" : ""
	projectilescale(float) { group = "Stats" } : "Projectile Scale" : "0.25" : ""
	projectilespeed(float) { group = "Stats" } : "Projectile Speed" : "2000" : ""
	reloadtime(float) { group = "Stats" } : "Reload Time" : "5" : ""
	reserveammo(integer) { group = "Stats" } : "Reserve Ammo" : 0 : ""
	secondaryrate(float) { group = "Stats" } : "Secondary Rate" : 0 : ""
	spread(float) { group = "Stats" } : "Spread" : "0" : ""
	buyable(boolean) { group = "Important" } : "Buyable" : 0 : ""
	exclusivefor(choices) { group = "Important" } : "Exclusive For" : 0 : "" =
	[
		0 : "None"
		1 : "Blue"
		2 : "Red"
	]
	holdtype(integer) { group = "Important" } : "Hold Type" : 0 : ""
	libraryname(string) { group = "Important" } : "Library Name" :  : ""
	slot(choices) { group = "Important" } : "Slot" : 0 : "" =
	[
		0 : "Primary"
		1 : "Secondary"
		2 : "Melee"
		3 : "Utility"
		4 : "Deployable"
	]
	icon(string) { group = "UI" editor = "AssetBrowse(png)" } : "Icon" : "" : ""
	viewmodel(resource:vmdl) { group = "Models" } : "View Model" : "" : ""
	worldmodel(resource:vmdl) { group = "Models" } : "World Model" : "" : ""
	deploytime(float) { group = "Stats" } : "Deploy Time" : "0.6" : ""
	movementspeedmultiplier(float) { group = "Stats" } : "Movement Speed Multiplier" : "1" : ""
	price(integer) { group = "Stats" } : "Price" : 0 : ""
]

